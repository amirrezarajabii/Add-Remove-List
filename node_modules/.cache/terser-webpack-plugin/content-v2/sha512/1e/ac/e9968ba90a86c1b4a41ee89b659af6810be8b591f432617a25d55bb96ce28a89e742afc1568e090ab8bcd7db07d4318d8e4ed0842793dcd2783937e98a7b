ini-css-extract-plugin","import React, { Component } from \"react\";\nimport \"./App.css\";\n\nconst Student = {\n  fName:\"\",\n  lName:\"\",\n};\n\nexport default class App extends Component {\n  constructor(props) {\n    super(props);\n\n    this.state = {\n      fName: \"\",\n      lName: \"\",\n      items: [],\n    };\n  }\n\n  updateFirstName(event) {\n    this.setState({\n      fName: event.target.value,\n    });\n  }\n\n  updateLastName(event) {\n    this.setState({\n      lName: event.target.value,\n    });\n  }\n\n  handleClick() {\n    var items = this.state.items;\n    const student = Object.create(Student);\n    student.fName = this.state.fName;\n    student.lName = this.state.lName;\n\n    items.push(student);\n\n    this.setState({\n      items: items,\n      fName: \"\",\n      lName: \"\",\n    });\n  }\n\n  handleItemChanged(i, event) {\n    var items = this.state.items;\n   \n    items[i].fName = event.target.fName;\n    items[i].lName = event.target.lName;\n\n    this.setState({items: items,});\n  }\n\n  handleItemDeleted(i) {\n    var items = this.state.items;\n\n    items.splice(i, 1);\n\n    this.setState({\n      items: items,\n    });\n  }\n\n\n  renderRows() {\n    var context = this;\n\n    return this.state.items.map(function (o, i) {\n      return (\n        <tr key={\"item-\" + i}>\n           <td className=\"col-1\">\n            <input\n            className=\"NoBorder\"\n              type=\"number\"\n              value={i}\n              disabled\n              onChange={context.handleItemChanged.bind(context, i)}\n            />\n          </td>\n          <td className=\"col-4\">\n            <input\n              type=\"text\"\n              value={o.fName}\n              onChange={context.handleItemChanged.bind(context, i)}\n            />\n          </td>\n\n          <td className=\"col-4\">\n            <input\n              type=\"text\"\n              value={o.lName}\n              onChange={context.handleItemChanged.bind(context, i)}\n            />\n          </td>\n\n          <td className=\"ta-center\">\n            <button\n              className=\"btn del-btn\"\n              onClick={context.handleItemDeleted.bind(context, i)}\n            >\n              Delete\n            </button>\n          </td>\n        </tr>\n      );\n    });\n  }\n\n  render() {\n    return (\n      <div className=\"col-xl-8 mt-5 mainTable\">\n        <h1>List of students</h1>\n        <table className=\"table col-12\">\n          <tr className=\"bg-green\">\n            <th className=\"col-1\">#</th>\n            <th className=\"col-4\">Firstname</th>\n            <th className=\"col-5\">Lastname</th>\n            <th className=\"col-2 ta-center\">Action</th>\n          </tr>\n    \n            <tr>\n            <th>\n                <input className=\"NoBorder\"\n                  type=\"text\"\n                  disabled\n                />\n              </th>\n              <th>\n                <input\n                  type=\"text\"\n                  placeholder=\"Firstname\"\n                  value={this.state.fName}\n                  onChange={this.updateFirstName.bind(this)} \n                />\n              </th>\n\n              <th>\n                <input\n                  type=\"text\"\n                  placeholder=\"Lastname\"\n                  value={this.state.lName}\n                  onChange={this.updateLastName.bind(this)} \n                />\n              </th>\n\n              <th className=\"ta-center\">\n                <button\n                type=\"submit\"\n                  className=\"btn add-btn\"\n                  onClick={this.handleClick.bind(this)}\n                >\n                  Add item\n                </button>\n              </th>\n            </tr>\n          <tbody>{this.renderRows()}</tbody>\n        </table>\n      </div>\n    );\n  }\n}\n","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport './index.css';\nimport A